<?xml version="1.0" encoding="UTF-8"?>
<elements>
	<element name="$UI/system/components/justep/shake/shake" tag-name="div" text="摇一摇" icon="shake.png" display-property="xid" 
			 binding-component="$UI/system/components/justep/shake/shake"
			 parent-range="$UI/system/components/justep/window/window,$UI/system/components/justep/inVisibleCompContainer/inVisibleCompContainer"
			 component-type="inVisibleComp"  ><!-- 不可视组件 -->
		<properties>
			<property name="xid" text="编号" required="true" />
		</properties>
		<templates>
		   <template name="default"><![CDATA[ 
		     <div component="$UI/system/components/justep/shake/shake" style="widht:0;height:0;">
		     </div>
		   ]]></template>
		</templates>
		<events>
			<event name="onShakeSuccess" type="event" text="摇一摇成功" parameters="event"
				is-script="true" />
			<event name="onShakeFail" type="event" text="不支持摇一摇" parameters="event"
				is-script="true" />
		</events>
	</element>
    
</elements>