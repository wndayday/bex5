	<private>
		<chart type="|pie|pie-3d|">
			<series-set>
				<series chart-type="" ref="">
					<data categoryAxis="" numberAxis="" ref="" />				
					<config>
						<is-circular>true</is-circular>
						<no-data-message>无数据</no-data-message>						
						<background-color>#FFFFFF</background-color>
						<background-alpha>0.6f</background-alpha>
						<foreground-alpha>0.9f</foreground-alpha>

						<label-gap>0.05</label-gap>
						<label-font name="黑体" 	style="0" size="12"/>					
						<pie-label-style>{0} {2}</pie-label-style>						
						<pie-label-number-format>######</pie-label-number-format>	
						<pie-label-percent-format>##.00%</pie-label-percent-format>	
						
						
						<!-- 着重弹出区域 -->
						<explode-all-percent range="100" value="0.0D"/>
						<explode-percent index="0" value="0.0D"/>
						
						<!-- 热点部分 -->
						<href model="" title="" />
						<map-tool-tip-label-style>{0}: ({1}, {2})</map-tool-tip-label-style>
						<map-tool-tip-number-format>######</map-tool-tip-number-format>	
						<map-tool-tip-percent-format>##.00%</map-tool-tip-percent-format>		
													
					</config>	
				</series>
			</series-set>
		</chart>
