						<arc-dial-scale-tick-label-font name="黑体" style="0" size="12"/>
						
						<arc-dial-pointer-radius>0.82</arc-dial-pointer-radius>
						<arc-dial-pointer-color>#0000FF</arc-dial-pointer-color>
						
						<!--温度计-->
						<thermometer-range lower="0" upper="100"/>
						<thermometer-units></thermometer-units>
						
					</config>	
				</series>
			</series-set>
		</chart>		
		<chart type="|vertical-gantt|horizontal-gantt|">
			<series-set>
				<series chart-type="" ref="">
					<data title="" categoryAxis="" startDate="" endDate="" color="" ref=""/>		
					<config>
						<background-color>#FFFFFF</background-color>	
						<background-alpha>0.6f</background-alpha>
						<foreground-alpha>0.9f</foreground-alpha>	
						<date-format>yyyy-MM-dd</date-format>
						
						<category-title>时间</category-title>
						<category-title-color>#000000</category-title-color>
						<category-title-font name="黑体" style="0" size="12"/>	
						
						<category-tick-label-font name="黑体" style="0" size="12"/>		
						<category-tick-label-color>#000000</category-tick-label-color>	
						
						<!-- 字体偏移度  -->
						<category-item-label-angle>0</category-item-label-angle>
		
						<serie-title>任务</serie-title>							
						<serie-title-color>#000000</serie-title-color>
						<serie-title-font name="黑体" style="0" size="12"/>
						
						<serie double-axis="false" one-axis-location="TOP_OR_LEFT" two-axis-location="BOTTOM_OR_RIGHT" />	
						
						<serie-tick-label-font name="黑体" style="0" size="12"/>	
						<serie-tick-label-color>#000000</serie-tick-label-color>
						
						<href model="" title="" />
						
						<map-tool-tip-label-format>{0}: ({1}, {2})</map-tool-tip-label-format>	
						<map-tool-tip-date-format>yyyy-MM-dd</map-tool-tip-date-format>														     
						
					</config>	
				</series>
			</series-set>
		</chart>	
		<chart type="|radar|">
			<series-set>
				<series chart-type="" ref="">
					<data title="" categoryAxis="" numberAxis="" color="" ref="" autoData="false"/>  			
					<config>
						<background-color>#FFFFFF</background-color>	
						<background-alpha>0.6f</background-alpha>
						<foreground-alpha>0.9f</foreground-alpha>
						
						<label-font name="黑体" style="0" size="12"/>		
						<label-color>#000000</label-color>
						
						<!-- 刻度个数 -->
						<ticks-count>5</ticks-count>	
						
						<!-- 刻度值 与线条的距离 -->
						<gap>10</gap>  	
						
						<href model="" title="" />		
						
						<!-- 提示信息 {0}组 {1}分类轴 {2}数据值 -->
						<map-tool-tip-label-style>({0}, {1}) = {2}</map-tool-tip-label-style>	
						<!-- type : [number/date] yyyy-MM-dd -->
						<map-tool-tip-format type="number">######.00</map-tool-tip-format>		
							   
					</config>	
				</series>
			</series-set>
		</chart>			
		<chart type="|default|">		
		</chart>
	</private>

</charts>