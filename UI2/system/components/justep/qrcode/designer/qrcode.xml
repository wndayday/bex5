<?xml version="1.0" encoding="UTF-8"?>
<elements>
	<element name="$UI/system/components/justep/qrcode/qrcode" tag-name="div" icon="qrcode.png" text="二维码"
		binding-component="$UI/system/components/justep/qrcode/qrcode" component-type="formControl">
		<properties>
			<property name="xid" text="编号"  required="true"/>	
			<property name="bind-content" text="内容" editor-ref="jSExpressionEditor"/>
			<property name="colorDark" default-value="#000000" text="前景色"/>
			<property name="colorLight" default-value="#ffffff" text="背景色"/>
			<property name="style" text="样式" editor-ref="styleDialog" />
			<property name="class"  text="class" editor-ref="classSelector"/>
		</properties>
		<property-editors>
		</property-editors>		
		<toolbar>	
		</toolbar>
		<events>
		</events>
		<templates>
		   <template name="default"><![CDATA[ 
				<div component="$UI/system/components/justep/qrcode/qrcode" style="width:150px; height:150px">
				</div>
		   ]]></template>
		</templates>
	</element>
</elements>
