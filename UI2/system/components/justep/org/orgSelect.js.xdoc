	@name $UI/system/components/justep/org/orgSelect#selectFilter
	@attribute
	@property 
	@type string
	@description 选中过滤表达式
*/

/**
	@name $UI/system/components/justep/org/orgSelect#includeOrgKind
	@attribute
	@property 
	@type string
	@description 显示的组织类型，组织扩展名使用","分隔的字符串类型，如：.psm,.dpt
*/

/**
	@name $UI/system/components/justep/org/orgSelect#process
	@attribute
	@property 
	@type string
	@description process
*/

/**
	@name $UI/system/components/justep/org/orgSelect#activity
	@attribute
	@property 
	@type string
	@description activity
*/

/**
	@name $UI/system/components/justep/org/orgSelect#orgDialog
	@attribute 
	@type string
	@description 选择组织机构使用的对话框XID，缺省：内部自动创建
*/

/**
	@name $UI/system/components/justep/org/orgSelect#getOrgDlg
	@function
	@returns {OrgDialog} 组织机构选择框组件
	@description 获取组织机构选择框
*/

/**
	@name $UI/system/components/justep/org/orgSelect#getInput
	@function
	@returns {Input} Input组件
	@description 获取Input
*/

/**
	@name $UI/system/components/justep/org/orgSelect#getBtn
	@function
	@returns {Button} Button组件
	@description 获取Button
*/

/**
	@name $UI/system/components/justep/org/orgSelect#val
	@function
	@description 取值或者赋值，同Input组件的val
*/

/**
	@name $UI/system/components/justep/org/orgSelect#set
	@function
	@description 设置属性值
	@param {json} arg 属性值对
	<br/><b>结构如下：</b>
	<xmp> 
	{
		"title" : {string} 显示的标题
		"limit" : {number} 加载数据的list一页的记录数,>0的数，默认12
		"cascade": {boolean} 多选时是否级联选择
		"showFilter": {string} 组织显示节点过滤表达式
		"selectFilter": {string} 选中过滤表达式
		"multiSelection": {boolean} 是否多选
		"includeOrgKind": {string} 显示的组织类型，组织扩展名使用","分隔的字符串类型，如：.psm,.dpt
		"itemValue": {string} 对应bind-ref的org列，缺省：SA_OPOrg
		"itemLabel": {string} 对应bind-labelRef的org列，缺省：sName 
		"process": {string}
		"activity": {string}
	}
	</xmp>	
	@return {void} 
*/

/**
	@name $UI/system/components/justep/org/orgSelect#get
	@function
	@description 获取属性值
	@param {String} name 属性名称，取值范围："title"、"limit"、"cascade"、"showFilter"、"selectFilter"、"multiSelection"、"includeOrgKind"、"itemValue"、"itemLabel"、"process"、"activity"
	@return {Object} 
*/