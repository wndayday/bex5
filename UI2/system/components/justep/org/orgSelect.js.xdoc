/**
	@name $UI/system/components/justep/org/orgSelect
	@component 
	@extends $UI/system/lib/base/bindComponent
	@model UI2
	@description OrgSelect
*/

/**
	@name $UI/system/components/justep/org/orgSelect#xid
	@attribute 
	@type string
	@description 组件标示
*/

/**
	@name $UI/system/components/justep/org/orgSelect#bind-ref
	@attribute 
	@type string
	@description 引用
*/

/**
	@name $UI/system/components/justep/org/orgSelect#bind-labelRef
	@attribute 
	@type string
	@description label引用
*/

/**
	@name $UI/system/components/justep/org/orgSelect#itemValue
	@attribute
	@property 
	@type string
	@description 对应bind-ref的org列，缺省：SA_OPOrg
*/

/**
	@name $UI/system/components/justep/org/orgSelect#itemLabel
	@attribute
	@property 
	@type string
	@description 对应bind-labelRef的org列，缺省：sName
*/

/**
	@name $UI/system/components/justep/org/orgSelect#title
	@attribute
	@property 
	@type string
	@description 标题，缺省：'选择组织机构'
*/

/**
	@name $UI/system/components/justep/org/orgSelect#multiSelection
	@attribute
	@property 
	@type boolean
	@description 多选
*/

/**
	@name $UI/system/components/justep/org/orgSelect#limit
	@attribute
	@property 
	@type integer
	@description 加载数据的list一页的记录数,>0的数，默认12
*/

/**
	@name $UI/system/components/justep/org/orgSelect#cascade
	@attribute
	@property 
	@type boolean
	@description 多选时是否级联选择
*/

/**
	@name $UI/system/components/justep/org/orgSelect#showFilter
	@attribute
	@property 
	@type string
	@description 组织显示节点过滤表达式
*/

