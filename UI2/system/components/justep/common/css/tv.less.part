/** control-group e---------- **/
@-webkit-keyframes x-foucsable {
  from {
    opacity: 1;
  }
  50% {
    opacity: 1;
  }
  100% {
    opacity: 1;
  }
}
@keyframes x-foucsable {
  from {
    opacity: 1;
  }
  50% {
    opacity: 1;
  }
  100% {
    opacity: 1;
  }
}
.x-focusable {
  height: auto;
  border: 3px solid transparent !important;
}
.x-focused {
  border: #DD5600 solid 3px !important;
  box-sizing:border-box;
}